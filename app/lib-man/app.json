{
  "expo": {
    "name": "lib-man",
    "slug": "lib-man",
    "version": "1.0.0",
    "newArchEnabled": true,
    
      "scheme": "lib-man",
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload images if you login as admin"
        }
      ],
      ],
      
      "experiments": {
        // "typedRoutes": true
        "typedRoutes": true
          ,"tsconfigPaths": true
        
      },
    
    "orientation": "portrait",
    "icon": "./assets/icon-black.png",
    
     "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-black.png",
        "backgroundColor": "#000000"
      }
    }
  }
}
